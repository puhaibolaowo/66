.info{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 100rpx 0;
}
.info .avatarUrl{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.info .nickName{
  color: #fff;
  margin-top: 30rpx;
}
.cell{
  display: flex;
  justify-content: space-between;
  width: 85%;
  background-color: #fff;
  padding: 30rpx 20rpx;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  align-items: center;
}
.cell-left text{
  margin-left: 20rpx;
}
.cell-left{
  display: flex;
  align-items: center;
}
.cell-right{
  font-size: 50rpx;
}